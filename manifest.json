{
  "manifest_version": 3,
  "name": "Eclipse Tab",
  "version": "1.0.0",
  "description": "个人化新标签页浏览器扩展",
  "permissions": [
    "storage",
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}